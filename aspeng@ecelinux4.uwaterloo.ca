#include <stdio.h>
#include "wiringPi.h"
#include <math.h>

int main(void){
	
	wiringPiSetupGpio();
	
	int inputPins[3] = { 6, 13, 19 };
	int outputPins[3] = { 16, 20, 21 };
	
	for (int i = 0; i < 3; i++){
		pinMode(inputPins[i], INPUT);
	}
	for (int i = 0; i < 3; i++){
		pinMode(outputPins[i], OUTPUT);
	}
	while(1){
		for (int i = 0; i < 3; i++){
			printf(digitalRead(outputPins[i]));
			printf(" ");
		}
		printf("\n");
	}
	
	
}